{
	"name": "SortViz",
	"service": "sort_viz",
	"remoteUser": "geba",
	"dockerComposeFile": "docker-compose.yml",
	"workspaceFolder": "/home/geba/app",
	"customizations": {
		"vscode": {
		  "extensions": [
			"ms-python.python",
			"ms-python.vscode-pylance",
			"ms-python.black-formatter",
			"njpwerner.autodocstring",
			"ms-toolsai.jupyter",
			"ms-azuretools.vscode-docker"
		],
		  "settings": {
			"editor.formatOnSave": true,
			"terminal.integrated.shell.*": "/bin/bash",
			"python.venvFolders": ["/opt/python/venv"]
		  }
		}
	  },
	"features": {
		"python": "3.10"
	}
}